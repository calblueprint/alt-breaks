.cover-wrapper
  - if CoverPhoto.team
    .cover-photo{:style => "background-image: url(#{CoverPhoto.team.photo.url(:large)})" }
  .title-container
    .title
      Team

#team
  .container
    .row-fluid
      .heading.center
        Staff
      .section
        - @staff.each do |m|
          = render 'page/director', :member => m

    .row-fluid
      .heading.center
        Directors
      .section
        - @directors.each do |m|
          = render 'page/director', :member => m
    .row-fluid
      .heading.center
        Break Leaders
      .section
        - @break_leaders.each do |m|
          = render 'page/break_leader', :member => m
    .row-fluid


