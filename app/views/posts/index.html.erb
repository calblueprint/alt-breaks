<div class="row-fluid">
<div class="span4 offset1"><h1>Poverty in LA</h1></div>
<div class="row-fluid">
<div class="span2 offset9"><%= link_to "Create Post", new_post_path, :class=>"btn" %></div>
<div class="row-fluid">
<div class="span1 offset11"></div>
<div class="row-fluid">
<div class="span10 offset1">
<table class="table table-hover table-bordered">
  <% @posts.each do |post| %>
  </tbody>
    <td><h4><%= link_to post.title, post_path(post.id) %></h4> <br> Created by: <%= post.user.name %></td>

  </tbody>
  <% end %>
 </table>
<div class="row-fluid">
<div class="span2"><%= link_to "GO HOME", root_path, :class=>"btn" %></div>
</div>
</div>
</div>
</div>
</div>

