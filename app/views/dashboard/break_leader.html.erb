<h1>Break Leader Dashboard</h1>
<div class="row-fluid">
<div class="offset1">
<h3>My Group Members<h3>
</div>
</div>
<div class="row-fluid">
<div class="span10 offset1">
<table class="table table-hover">
  <% @list_of_members.each do |member| %>
  </tbody>
    <td><%= member.name %></td>
  </tbody>
  <% end %>
 </table>
</div>
</div>
<div class="row-fluid">
<div class="span10 offset1">
<h3> Pending Requests</h3>
<table class="table table-hover">
  <% @requests_for_break_leader.each do |request| %>
  </tbody>
    <td><%= request.user.name %></td>
    <td><%= link_to "Approve", approve_request_path(request.id), :class=>"btn btn-success" %></td>
  </tbody>
  <% end %>
 </table>
</div>
</div>
